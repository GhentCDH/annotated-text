    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@nx/devkit": "21.4.1",
    "@nx/js": "21.4.1",
    "@nx/playwright": "21.4.1",
    "@nx/rollup": "21.4.1",
    "@nx/vite": "21.4.1",
    "@nx/vue": "21.4.1",
    "@nx/web": "21.4.1",
    "@playwright/test": "^1.36.0",
    "@rushstack/eslint-patch": "^1.11.0",
    "@swc-node/register": "~1.9.1",
    "@swc/cli": "~0.6.0",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
