    "@types/markdown-it": "14.1.2",
    "@types/memoizee": "0.4.12",
    "@types/node": "^22.14.1",
    "@types/semver": "7.7.0",
    "@types/uuid": "^10.0.0",
    "@types/yargs": "17.0.33",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "3.1.1",
    "@vitest/eslint-plugin": "^1.1.42",
    "@vitest/ui": "^3.0.0",
