{
  "name": "@ghentcdh/annotated-text",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GhentCDH/annotated-text"
  },
  "version": "0.0.2-1",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/annotated-text.js",
      "require": "./dist/annotated-text.cjs"
    },
    "./style.css": {
      "import": "./dist/annotated-text.css"
    }
  },
  "type": "module",
  "style": "./dist/annotated-text.css",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "nx serve",
    "preview": "nx vite:preview",
    "docs:build:typedoc": "typedoc",
    "docs:build:vuepress": "vuepress build docs ",
    "docs:build:menu": "ts-node --project tools/tsconfig.tools.json tools/doc/vuepress.mjs",
    "docs:build": "pnpm run docs:build:typedoc &&  pnpm run docs:build:menu && pnpm run docs:build:vuepress",
    "docs:preview": "vuepress dev docs",
    "docs:dev": "pnpm run docs:build:typedoc && pnpm run docs:build:menu && vuepress dev docs",
    "test:vue3": "nx test --environment jsdom --root src/",
    "test:coverage": "nx test --environment jsdom --root src/ --coverage",
    "type-check": "vue-tsc --noEmit --composite false",
    "prepare": "husky install",
    "release": "node tools/release/release.mjs"
  },
  "dependencies": {
    "d3": "7.9.0",
    "lodash-es": "^4.17.21",
    "markdown-it": "14.1.0",
    "memoizee": "^0.4.17",
    "tokenizr": "1.7.0",
    "zod": "3.25.67"
  },
  "devDependencies": {
    "@babel/types": "^7.27.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
