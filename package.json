{
  "name": "@ghentcdh/vue-component-annotated-text",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/GhentCDH/vue_component_annotated_text"
  },
  "version": "0.0.17",
  "exports": {
    ".": {
      "import": "./dist/vue-component-annotated-text.js",
      "require": "./dist/vue-component-annotated-text.cjs",
      "types": "./dist/index.d.ts"
    },
    "./style.css": {
      "import": "./dist/style.css"
    }
  },
  "type": "module",
  "style": "./dist/style.css",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "vite",
    "preview": "vite preview",
    "docs:build": "typedoc --plugin typedoc-plugin-markdown --plugin typedoc-plugin-vue --entryPoints \"./src/lib/annotatedTextUtils/**.ts\" --entryPoints \"./src/types/**.ts\" --out docs/typedoc",
    "test:vue3": "vitest --environment jsdom --root src/",
    "test:coverage": "vitest --environment jsdom --root src/ --coverage",
    "build": "vite build",
    "type-check": "vue-tsc --noEmit --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "etali": "^1.2.5",
    "pinia": "^2.2.2"
  },
  "devDependencies": {
    "@babel/types": "^7.25.6",
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.6.0",
    "@fortawesome/free-regular-svg-icons": "^6.6.0",
    "@fortawesome/free-solid-svg-icons": "^6.6.0",
    "@fortawesome/vue-fontawesome": "^3.0.8",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^18.19.50",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitest/coverage-c8": "^0.31.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prism-themes": "^1.9.0",
    "sass": "^1.78.0",
    "stylelint": "^15.11.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "typedoc": "^0.26.7",
    "typedoc-plugin-markdown": "^4.2.7",
    "typedoc-plugin-vue": "^1.2.0",
    "typescript": "^5.0.0",
    "uuid": "^10.0.0",
    "vite": "^4.5.3",
    "vite-plugin-dts": "^2.3.0",
    "vitepress": "^1.3.4",
    "vitest": "^0.31.4",
    "vue": "^3.5.6",
    "vue-docgen-cli": "^4.79.0",
    "vue-live": "^2.5.4",
    "vue-tsc": "^1.8.27"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  }
}
